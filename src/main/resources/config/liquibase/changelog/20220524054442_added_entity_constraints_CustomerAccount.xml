<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity CustomerAccount.
    -->
    <changeSet id="20220524054442-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="bank_id"
                                 baseTableName="customer_account"
                                 constraintName="fk_customer_account__bank_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="customer_account"
                                 constraintName="fk_customer_account__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>
    </changeSet>
</databaseChangeLog>
